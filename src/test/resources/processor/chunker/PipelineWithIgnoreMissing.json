{
  "description": "An example fixed token length chunker pipeline with ignore missing == true",
  "processors" : [
    {
      "text_chunking": {
        "ignore_missing": true,
        "field_map": {
          "body": "body_chunk"
        },
        "algorithm": {
          "fixed_token_length": {
            "token_limit": 10,
            "tokenizer": "letter"
          }
        }
      }
    }
  ]
}
